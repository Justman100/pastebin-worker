{
  "private": true,
  "name": "pb",
  "version": "1.0.0",
  "description": "Pastebin based on Cloudflare worker",
  "repository": "https://github.com/SharzyL/pastebin-worker",
  "type": "module",
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev --var BASE_URL:http://localhost:8787 --port 8787",
    "build": "wrangler deploy --dry-run --outdir=dist",
    "delete-paste": "wrangler kv:key delete --binding PB",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "author": "SharzyL <me@sharzy.in>",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.5.7",
    "@vitest/coverage-istanbul": "2.1.1",
    "vitest": "2.1.1",
    "eslint": "^9.11.0",
    "prettier": "^3.3.3",
    "wrangler": "^3.78.7"
  },
  "dependencies": {
    "mdast-util-to-string": "^4.0.0",
    "mime": "^3.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "unified": "^11.0.5",
    "vfile-message": "^4.0.2"
  },
  "resolutions": {
    "micromark": "^3.1.0"
  }
}